{
  "name": "framework-mobile",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --API=dev",
    "build": "vue-cli-service build --API=production",
    "lint": "vue-cli-service lint",
    "build:test": "vue-cli-service build --API=test"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^3.4.3",
    "framework7": "^5.2.0",
    "framework7-vue": "^5.2.0",
    "lodash": "^4.17.15",
    "qs": "^6.9.1",
    "vant": "^2.4.0",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-plugin-vuex": "^4.1.2",
    "@vue/cli-service": "^4.1.0",
    "@vue/eslint-config-prettier": "^5.0.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-import": "^1.13.0",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "prettier": "^1.19.1",
    "vue-cli-plugin-postcss-preset": "^1.0.2",
    "vue-template-compiler": "^2.6.10",
    "webpack-bundle-analyzer": "^3.6.0",
    "yargs-parser": "^18.1.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "globals": {
      "__DEV__": true
    },
    "extends": [
      "plugin:vue/strongly-recommended",
      "@vue/prettier"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "postcss-preset-env": {
        "stage": 2
      }
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "sideEffects": [
    "*.css",
    "*.less",
    "*.vue"
  ]
}
